        "get-audio-duration": "^1.0.0",
        "prettier": "^1.19.1",
        "ts-node": "^8.6.2",
        "typescript": "^3.7.5"
    },
    "dependencies": {
        "csv-parse": "^4.8.5",
        "csvtojson": "^2.0.10",
        "deep-equal": "^2.0.1",
        "dockerode": "^3.1.0",
        "execa": "^4.0.0",
        "js-yaml": "^3.13.1",
